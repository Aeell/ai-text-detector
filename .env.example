# Application
NODE_ENV=development
PORT=3000

# Analytics
ANALYTICS_ENDPOINT=https://analytics.example.com/collect
ANALYTICS_API_KEY=your_analytics_api_key

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_DEBUG=true
ENABLE_CACHE=true

# Performance
MAX_CACHE_SIZE=100
BATCH_SIZE=50
REQUEST_TIMEOUT=5000

# Security
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com
API_RATE_LIMIT=100
API_RATE_WINDOW=900 